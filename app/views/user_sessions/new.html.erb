<% title "Login" %>

<%= form_for @user_session do |f| %>
  <%#= f.error_messages %>
   <%= errors_for(@user_session, "Opps, unable to add new user").html_safe %>  
  <p>
    <%= f.label :username %><br />
    <%= f.text_field :username %>
  </p>
  <p>
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </p>
  <p><%= f.submit "Login" %></p>
<% end %>


